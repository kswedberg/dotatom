"*":
  "atom-alignment":
    addSpacePostfix: true
  "atom-beautify":
    _analyticsUserId: "05c56071-6859-4f96-bcb2-992bc66f7e33"
    analytics: false
    beautifyEntireFileOnSave: false
    html_end_with_newline: true
    js_end_with_newline: true
    js_keep_array_indentation: true
    js_max_preserve_newlines: 2
    language_json_beautify_on_save: true
    language_php_disabled: true
  "auto-update-packages": {}
  "autocomplete-plus":
    confirmCompletion: "enter"
    useAlternateScoring: true
  autosave: {}
  blame: {}
  "coffee-compile": {}
  core:
    audioBeep: false
    customFileTypes:
      "source.js.jsx": [
        "js"
        "jsx"
      ]
    disabledPackages: [
      "markdown-preview"
      "autocomplete-emojis"
      "pane-free"
      "auto-update-packages"
      "language-gfm"
      "linter-phpcs"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".git"
      ".svn"
      ".DS_Store"
      ".sass-cache"
    ]
    packagesWithKeymapsDisabled: [
      "es6-javascript"
      "tag"
      "coffee-compile"
    ]
    themes: [
      "atom-dark-ui"
      "one-light-syntax"
    ]
  docblockr: {}
  editor:
    invisibles: {}
    preferredLineLength: 100
    showIndentGuide: true
    softWrap: true
    softWrapHangingIndent: 2
  emmet:
    extensionsPath: "~/bin/emmet"
  "exception-reporting":
    userId: "24b42741-85cc-662e-ac4a-2329d5e7557c"
  "find-and-replace":
    openProjectFindResultsInRightPane: true
  "fuzzy-finder":
    ignoredNames: [
      ".sass-cache"
    ]
    preserveLastSearch: true
  "git-blame":
    ignoreWhiteSpaceDiffs: false
  "git-plus": {}
  "js-autoimport":
    trailingSemicolon: true
  "language-babel": {}
  linter:
    errorPanelHeight: 120
    lintOnFlyInterval: 800
    showErrorInline: false
    showErrorTabLine: true
  "linter-annotations": {}
  "linter-eslint":
    useGlobalEslint: true
  "linter-jscs":
    displayAs: "jscs Warning"
    esnext: true
    fixOnSave: true
    onlyConfig: true
    preset: "grunt"
  "linter-jshint":
    disableWhenNoJshintrcFileInPath: true
    lintJSXFiles: true
  "linter-jsxhint":
    harmony: true
  "linter-php":
    executablePath: "/usr/local/bin/php"
  "linter-phpcs":
    codeStandardOrConfigFile: "/Users/kswedberg/phpcs-rules.xml"
    executablePath: "/usr/local/bin/phpcs"
  "linter-tidy":
    tidyExecutablePath: "/usr/local/bin/tidy"
  "markdown-preview":
    grammars: [
      "source.gfm"
      "source.litcoffee"
      "text.plain"
      "text.plain.null-grammar"
    ]
  "markdown-preview-plus":
    grammars: [
      "text.md"
      "source.gfm"
      "text.html.basic"
      "text.plan"
      "text.plain.null-grammar"
    ]
    liveUpdate: false
    useGitHubStyle: true
  "markdown-writer":
    fileExtension: ".md"
    grammars: [
      "text.md"
      "source.gfm"
      "source.litcoffee"
      "text.plain"
      "text.plain.null-grammar"
    ]
  metrics:
    userId: "2c3c1c2ee7e96b8eb3f2365024a40f726b1578d6"
  "one-dark-ui": {}
  "one-light-ui": {}
  "php-cs-fixer":
    executablePath: "/usr/local/bin/php-cs-fixer"
    executeOnSave: true
    fixers: "align_double_arrow,concat_with_spaces,elseif,function_call_space,indentation"
  react: {}
  "release-notes":
    viewedVersion: "0.94.0"
  tabs: {}
  tag: {}
  "tree-ignore": {}
  "tree-view":
    hideIgnoredNames: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
    removeTrailingWhitespace: true
  "word-jumper-deluxe": {}
